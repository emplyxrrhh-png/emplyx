    if (isEditMode) {
      setIsLoading(true);
      fetch(`https://localhost:5001/api/users/${id}`).then((res) => {
        if (!res.ok) throw new Error("Failed to fetch user");
        return res.json();
      }).then((data) => {
        setFormData({
          id: data.id,
          username: data.userName,
          email: data.email,
          fullName: data.displayName,
          isActive: data.isActive,
          roles: data.roles.map((r) => r.rolId)
        });
